@model IEnumerable<ThAmCo.Events.Data.GuestBooking>

    <h2>Event List</h2>

    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Event.Title)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Event.Date)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Attended)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Event.Title)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Event.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Attended)
                </td>
                <td>
                    @Html.ActionLink("Details", "Details", "Event", new { id = item.EventId })
                </td>
            </tr>
            }
        </tbody>
    </table>
